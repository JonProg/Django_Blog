{
    "builds": [
      {
        "src": "vercel_app/wsgi.py",
        "use": "@vercel/python"
      }
    ],

    "routes": [
      {
        "src": "/(.*)",
        "dest": "vercel_app/wsgi.py"
      },
      
      {
        "src": "/static/(.*)",
        "dest": "/data/web/static/$1"
      }

    ]
    
}